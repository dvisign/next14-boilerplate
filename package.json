{
  "name": "hana-bolierplate",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:web":"turbo run build --filter=./apps/web",
    "build:admin":"turbo run build --filter=./apps/admin",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=!./apps/admin",
    "dev:admin": "turbo dev --filter=!./apps/web",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "graph": "turbo run build lint --graph=graph.png"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "babel-loader": "^9.1.3",
    "@reduxjs/toolkit": "^1.9.5",
    "autoprefixer": "^10.4.17",
    "react-redux": "^8.1.1",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "sass": "^1.69.7",
    "@mui/base": "^5.0.0-beta.34",
    "@mui/material": "^5.15.3",
    "@mui/system": "^5.15.7",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "clsx": "^2.1.0",
    "axios": "^1.6.5",
    "lodash": "^4.17.21",
    "eslint-config-next": "^14.1.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@emotion/babel-plugin": "^11.11.0",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-redux": "^7.1.33",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "turbo": "latest",
    "@mswjs/http-middleware": "^0.9.1",
    "msw": "^2.2.0",
    "@vercel/style-guide": "^5.1.0",
    "eslint-config-turbo": "^1.11.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.6.15",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
